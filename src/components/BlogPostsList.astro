---
import BlogPostCard from "./BlogPostCard.astro";
import { uniquefyBlogProp, type Post } from "../pages/posts/[...slug].astro";
interface Props {
    posts: Post[];
}
const { posts } = Astro.props;
---

<ul class="flex flex-col gap-2 items-center w-full">
    {posts.map((post) => <BlogPostCard post={post} propSlugifier={uniquefyBlogProp(post)} />)}
</ul>
